{
  "expo": {
    "name": "aura-english",
    "slug": "aura-english",
    "version": "1.3.0",
    "orientation": "portrait",
    "scheme": "auraenglish",
    "plateforms": [
      "android"
    ],
    "userInterfaceStyle": "automatic",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.allmaxou.auraenglish",
      "versionCode": 3
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-sqlite",
      "expo-secure-store",
      "expo-audio"
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "df405ce5-236d-414d-9528-1fe50de60015"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/df405ce5-236d-414d-9528-1fe50de60015",
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    }
  }
}
